<div class="form-group">
    <label class="col-sm-2 control-label" for="ifthenpayPayshopKey">{{ entry_payshop_payshopKey }}</label>
    <div class="col-sm-10">
        <select name="payment_ifthenpay_payshop_payshopKey" id="ifthenpayPayshopKey" class="form-control">
        {% if payshop_payshopKeys %}
            {% for payshop_payshopKey in payshop_payshopKeys %}
                <option value="{{ payshop_payshopKey.value }}">{{ payshop_payshopKey.value }}</option>
            {% endfor %}
        {% else %}
            <option value="{{ payment_ifthenpay_payshop_payshopKey }}" selected="selected">{{ payment_ifthenpay_payshop_payshopKey }}</option>
        {% endif %}
        </select>
        {{ spinner }}
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label" for="ifthenpayPayshopValidade">{{ entry_payshop_validade }}</label>
    <div class="col-sm-10">
        <input type="text" name="payment_ifthenpay_payshop_validade" value="{{ payment_ifthenpay_payshop_validade }}"  id="ifthenpayPayshopValidade" class="form-control" />
        <small id="sandboxHelp" class="form-text text-muted">{{payshop_validade_helper}}</small>
    </div>
</div>
{% if not payshop_payshopKeys %}
    {{ifthenpay_chooseNewEntidadeBtn}}
{% endif %}